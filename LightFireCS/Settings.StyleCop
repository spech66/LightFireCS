      <AnalyzerSettings>
        <StringProperty Name="CompanyName">Sebastian Pech</StringProperty>
        <StringProperty Name="Copyright">Copyright (C) Sebastian Pech</StringProperty>
      </AnalyzerSettings>
    </Analyzer>
